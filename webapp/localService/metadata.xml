<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<edmx:Edmx Version="1.0" 
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
	<edmx:DataServices 
		xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
		m:DataServiceVersion="1.0" 
		m:MaxDataServiceVersion="3.0">
		<Schema Namespace="EmployeeModel" 
			xmlns="http://schemas.microsoft.com/ado/2008/09/edm"
			xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
            xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
			<EntityType Name="Employee">
				<Key>
					<PropertyRef Name="EID"/>
				</Key>
				<Property Name="EID" Type="Edm.String" Nullable="false"/>
				<Property Name="Enabled" Type="Edm.Boolean" Nullable="false"/>
                <Property Name="Address" Type="EmployeeModel.Address" Nullable="false"/>
				<Property Name="SalesAmount" Type="Edm.Decimal" Precision="10" Scale="2" Nullable="false"/>
				<Property Name="CurrencyCode" Type="Edm.String" MaxLength="3" Nullable="false"/>
			</EntityType>
            <ComplexType Name="Address">
                <Property Name="Street" Type="Edm.String"/>
                <Property Name="City" Type="Edm.String"/>
                <Property Name="Zip" Type="Edm.String"/>
                <Property Name="Country" Type="Edm.String"/>
            </ComplexType>
			<EntityContainer Name="EmployeeEntities" m:IsDefaultEntityContainer="true">
				<EntitySet Name="Employees" EntityType="EmployeeModel.Employee"/>
				<FunctionImport Name="FindUpcomingEmployees" EntitySet="Employees" ReturnType="Collection(EmployeeModel.Employee)" m:HttpMethod="GET" />
			</EntityContainer>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>