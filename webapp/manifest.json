{
  "_version": "1.65.0",
  "sap.app": {
    "id": "com.ui5.trng.sapui5bootcampactivity05",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "453e928b-fd83-47a9-a570-288fbd4280f0"
    },
    "dataSources": {
			"mockdata": {
				"uri": "/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.139.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.ui5.trng.sapui5bootcampactivity05.i18n.i18n"
        }
      },
      "productsModel": { 
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Products.json"
      },
      "": {
				"dataSource": "mockdata"
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.ui5.trng.sapui5bootcampactivity05.view",
        "async": true,
        "viewPath": "com.ui5.trng.sapui5bootcampactivity05.view"
      },
      "routes": [
        {
          "name": "RouteJSONBinding",
          "pattern": ":?query:",
          "target": [
            "TargetJSONBinding"
          ]
        }
      ],
      "targets": {
        "TargetJSONBinding": {
          "id": "JSONBinding",
          "name": "JSONBinding"
        }
      }
    },
    "rootView": {
      "viewName": "com.ui5.trng.sapui5bootcampactivity05.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
